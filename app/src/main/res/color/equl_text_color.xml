<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:color="@color/material_on_surface_stroke">
        <shape>
            <solid
                android:color="#5b99bf" />

        </shape>
    </item>
    <item android:color="@color/material_on_primary_emphasis_medium">
